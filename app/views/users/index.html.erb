<h1>Users List:</h1>

    <ul>
        <% @users.each do |user| %>
            <li><%= link_to user.username, user_path(user) %></li>
        <% end %>
    </ul>

    <% if logged_in? %>
        <%= link_to 'Log Out', logout_path, method: 'post' %><br>
    <% end %> 

    <% if logged_in? %>
        <%= link_to 'Home', root_path %><br>
    <% end %>
     