<h2><%= current_user.username %> Contacts</h2>

    <ul>
    <%= @users.each do |user| %>
        <li><%= link_to user.username, users_path(user) %></li>
    <% end %>
    </ul>


    <% if logged_in? %>
        <%= link_to 'Log Out', logout_path, method: 'post' %><br>
    <% end %> 

    <% if logged_in? %>
        <%= link_to 'Home', root_path %><br>
    <% end %>
     