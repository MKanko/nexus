<h2>Welcome <%= @user.username %></h2>

    <h3>Your Contacts:</h3>

        <ul>
            <% @user.contacts.each do |con| %>
                <li><%= link_to con.name, contact_path(con) %></li>
            <% end %>
        </ul>

    

    <% if logged_in? %>
        <%= link_to 'Edit User', edit_user_path %><br> 
    <% end %>

    <% if logged_in? %>
        <%= link_to 'Create Contact', new_contact_path %><br> 
    <% end %>

    <% if logged_in? %>
        <%= link_to 'Contacts', contacts_path %><br> 
    <% end %>

    

