<%= form_for @schedule do |f| %>
    <%= f.label :User %>
    <%= f.text_field :user_username %><br><br>

    <%= f.label :name %>
    <%= f.text_field :name %><br><br>

    <%= f.label :year %>
    <%= f.text_field :year %><br><br>

    <%= f.label :month %>
    <%= f.text_field :month %><br><br>
    
    <%= f.label :day_date %>
    <%= f.text_field :day_date %><br><br>

    <%= f.label :day_name %>
    <%= f.text_field :day_name %><br><br>

    <%= f.label :hour %>
    <%= f.text_field :hour %><br><br>

    <%= f.label :half_hour %>
    <%= f.text_field :half_hour %><br><br>

    <%= f.label :schedule_item %>
    <%= f.text_field :schedule_item %><br><br>

    <%= f.submit 'Submit Schedule' %>
<% end %>