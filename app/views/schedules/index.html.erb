<h3><%= current_user.username %></h3>

    <%= current_date %>

    <h4>Your Schedules:</h4>

        <div class='ui blue segment sched-seg'
            <ul>
                <% current_user.schedules.each do |sched| %>
                    <div class='ui blue card'><%= link_to sched.name, schedule_path(sched) %></div>
                <% end %>
            </ul>
        </div>
    